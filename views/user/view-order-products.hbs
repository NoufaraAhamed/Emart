<section>

        <div class="container mt-5">
            <div class="row">

                {{#each products}}
                <div class="col-md-3 p-5 productParentDiv">
                    <div class="card">
                        <img class="card-img-top img"
                            src="/product-images/{{this.product._id}}.jpg"
                            alt="Card image cap"
                        >
                        <div class="card-body">
                            <h5 class="card-title">{{this.product.Name}}</h5>
                            <p class="cart-text">$.{{this.product.price}}</p>
                            <p class="card-text">Quantity:{{this.quantity}}</p>
                           
                            
                        </div>
                    </div>
                </div>

                {{/each}}
            </div>
        </div>

    </section>

    